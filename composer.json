{
    "name": "1sept/oauth2-1sept",
    "description": "September First OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Vasilyev",
            "email": "a.vasilyev@1sept.ru"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authorization",
        "1sept"
    ],
    "require": {
        "php": "^7.3 || ^8",
        "league/oauth2-client": "^2.0"
    },
    "require-dev": {
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpdoc-parser": "^1.29",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-strict-rules": "^1.5",
        "phpunit/phpunit": "^11.1"
    },
    "autoload": {
        "psr-4": {
            "Sept\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sept\\OAuth2\\Client\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "scripts": {
        "test": [
            "@phpunit",
            "@phpstan"
        ],
        "phpunit": "vendor/bin/phpunit",
        "phpstan": "vendor/bin/phpstan"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
